  },
  "main": "./lib/typescript.js",
  "typings": "./lib/typescript.d.ts",
  "bin": {
    "tsc": "./bin/tsc",
    "tsserver": "./bin/tsserver"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "devDependencies": {
    "@types/browserify": "latest",
