  "scripts": {
    "pretest": "jake tests",
    "test": "jake runtests-parallel",
    "build": "npm run build:compiler && npm run build:tests",
    "build:compiler": "jake local",
    "build:tests": "jake tests",
    "start": "node lib/tsc",
    "clean": "jake clean",
    "gulp": "gulp",
    "jake": "jake",
    "lint": "jake lint",
